lay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"map-wrapper\">\n                    <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n                </div>\n                <div class=\"items-wrapper\" mbr-if=\"showContacts\">\n                    \n                    \n                    <div class=\"item features-without-image active\">\n                        <div class=\"item-wrapper\">\n                            <div class=\"card-box card\">\n                                <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title, .icon-wrapper\" mbr-if=\"showTitle\"><b>Locatie</b></h4>\n                                <div class=\"desc-wrap\">\n                                    <a href=\"#\">\n                                        <p class=\"item-desc mbr-fonts-style\" mbr-if=\"showDesc\" mbr-theme-style=\"display-7\" data-app-selector=\".item-desc, .desc-wrap\">De klomp 123, Enschede</p>\n                                    </a>\n                                </div>\n                            </div>\n                        </div>\n                    </div><div class=\"item features-without-image\">\n                        <div class=\